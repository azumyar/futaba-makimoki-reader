<prism:PrismApplication
    x:Class="Yarukizero.Net.MakiMoki.Reader.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:prism="http://prismlibrary.com/"
    xmlns:sys="clr-namespace:System;assembly=mscorlib"
    xmlns:local="clr-namespace:Yarukizero.Net.MakiMoki.Reader">
    <prism:PrismApplication.Resources>
        <ResourceDictionary>
            <FontFamily x:Key="MakimokiUiFont">Yu Gothic UI,Meiryo UI</FontFamily>
            <FontFamily x:Key="MakimokiSymbolFont">Segoe Fluent Icons,Segoe MDL2 Assets,Segoe UI Symbol</FontFamily>
        </ResourceDictionary>
    </prism:PrismApplication.Resources>
</prism:PrismApplication>
